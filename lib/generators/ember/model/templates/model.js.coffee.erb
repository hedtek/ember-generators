<%= application_name %>.Models.<%= model_name %> = Ember.Resource.define
  url: '<%%= r.<%= route_name %>%>'
  <%- unless schema.empty? %>
  schema:<%- schema.each do |name, type| %>
    <%= name %>: <%= type %> <%- end %>
  <%- else %>
  schema: {}
  # Usage:
  #
  # schema:
  #   attr1: Number
  #   attr2: String
  <%- end %>
